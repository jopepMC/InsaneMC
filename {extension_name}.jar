package org.insanemc.extension.{extension_name};

import org.insanemc.core.extension.Extension;

/**
 * Copyright (c) InsaneMC 12-01-2018.
 * Created by jopep on 16:59.
 */
public class {extension_name} extends Extension {

    @Override
    public void onEnable() {
        // To create a config same as spigot
        getConfig().options().copyDefaults(true);
        saveConfig();
    }

    @Override
    public void onDisable() {

    }

    @Override
    public Listener[] listeners() {
        return new Command[]{new {listener_class}, ...};
    }

    @Override
    public Command[] commands() {
        return new Command[]{new {command_class}, ...};
    }
}

